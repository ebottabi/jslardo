h3 #{__i('models')}
	img(src='/images/pov/icon_core_jslModel_60x45.png').floatLeft
hr
include ../includes/paginationTop
include ../includes/filterAllOrMine
include ../includes/listCrudbox

each element,i in elements
	div.floatLeft.size1
		//- ok ma va al dettaglio a(href='/#{elementName}s/#{element._id}').elemGrid.outerCont
		a(href='/contents/#{element._id}', title='#{__i('contents for this model')}').elemGrid.outerCont
			h5 #{element.name}
			//-
				div.innerCont
					span.textMini #{__i('domain')}: 
						h6 #{element.domain}
			include ../includes/listInfoBox
	if (i+1)%6==0
		div.clear
include ../includes/paginationBottom
